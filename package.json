{
  "name": "eslint-plugin-diff",
  "version": "1.0.12-2",
  "description": "Run ESLint on your changes only",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "diff"
  ],
  "homepage": "https://github.com/paleite/eslint-plugin-diff#readme",
  "bugs": {
    "url": "https://github.com/paleite/eslint-plugin-diff/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleite/eslint-plugin-diff.git"
  },
  "license": "MIT",
  "author": "Patrick Eriksson <paleite@users.noreply.github.com> (https://github.com/paleite)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    ".github/ISSUE_TEMPLATE/feature_request.md",
    "/dist/",
    "CODE_OF_CONDUCT.md",
    "SECURITY.md"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean",
    "format": "prettier --write .",
    "lint": "eslint --cache --ext .js,.ts --fix --format codeframe .",
    "release": "np",
    "test": "jest --coverage",
    "typecheck": "tsc --project tsconfig.json",
    "prepublish": "yarn run clean && yarn run build",
    "postpublish": "pinst --enable",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.8",
    "@types/eslint": "^7.28.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-files": "^1.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "np": "^7.5.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "eslint": ">=6.7.0"
  },
  "engines": {
    "node": ">=10.23.0"
  }
}
